{
  "name": "family-monitoring-be",
  "version": "1.0.0",
  "description": "Expense Tracking: The process of recording and monitoring the various expenses incurred by a family or individual. It involves keeping a detailed record of expenditures, categorizing them, and analyzing spending patterns.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d dist",
    "dev": "npm run build && nodemon dist/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "make-seed": "npx sequelize-cli seed:generate --name",
    "seed": "npx sequelize-cli db:seed:all",
    "_seed": "npx sequelize-cli db:seed --seed",
    "unseed": "npx sequelize-cli db:seed:undo:all",
    "_unseed": "npx sequelize-cli db:seed:undo --seed",
    "down": "npx sequelize-cli db:migrate:undo:all",
    "_down": "npx sequelize-cli db:migrate:undo --to"
  },
  "author": "",
  "license": "ISC"
}
